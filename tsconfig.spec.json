/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": [
      "jasmine"
    ],
    "strict": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts",
    "src/**/*.d.ts",
    "src/test.ts",
    "**/*.spec.ts"
  ]
}
